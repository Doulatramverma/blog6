 
 <button type="button" class="btn btn-info" data-toggle="collapse" data-target="#demo1">your friends</button> <br>
<div class="container">
<div id="demo1" class="collapse">
      
      <% if @friends !=nil %>
          <%  @friends.each do |friend| %>
           <%= blog.where(id: friend.sender).email %>
          <% end %>
      <% end %>

      <%if @friends1 !=nil%>
            <%  @friends1.each do |friend| %>
             <%= blog.where(id: friend.receiver).email %>
            <% end %>
      <% end %>
</div>
</div>

  <br><button type="button" class="btn btn-info" data-toggle="collapse" data-target="#demo">Add a friend</button>  
<div class="container">
<div id="demo" class="collapse">
  
<% @users.each do |user| %>
  <p>
  
    <%= user.name %>
    <%= link_to "Add Friend", blog_addfriend_path(id: user.id) %>
  </p>
 
<% end %>
<%if @friend_request !=nil%>

<%  @friend_request.each do |frr| %>
  <p>
     <%unless frr.accept==true&&frr !=nil%>
     <%@blogrr=blog.where(id: frr.sender).first%>
    <%= "u have a friend request from #{@blogrr.name }" %>
    <%= link_to "accept", user_acc_path(re: current_user.id,se: @blogrr.id) %>
    
  <%= link_to "delete", user_de_path(re: current_user.id,se: @blogrr.id) %>
    <%end%>
  </p>
 
  
<% end %>
<% end %>

 </div>
</div>





